{
  "name": "springtunes",
  "version": "0.1.7",
  "description": "Control your Spotify client remotely from your browser without having to log in to anything. (MACOS X 10.10+ only)",
  "author": "Vincent Audebert <vincent@springload.co.nz>",
  "contributors": [
    {
      "name": "Katie Day"
    },
    {
      "name": "Thibaud Colas"
    },
    {
      "name": "Josh Barr"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/springload/springtunes.git"
  },
  "engines": {
    "node": "^6",
    "npm": "^3"
  },
  "keywords": [
    "control",
    "music",
    "spotify",
    "remote",
    "redux",
    "client"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springload/springtunes/issues"
  },
  "homepage": "https://github.com/springload/springtunes",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "isomorphic-fetch": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.0",
    "node-spotify-webhelper": "0.0.4",
    "osa-spotify": "^1.0.1",
    "raw-loader": "^0.5.1",
    "rc-slider": "^3.3.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-svg-inline": "^1.2.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^2.0.1",
    "spotify-node-applescript": "^0.2.2",
    "uglifyjs": "^2.4.10"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.3",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "enzyme": "^2.6.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.6.0",
    "jest": "^17.0.2",
    "node-libs-browser": "^1.0.0",
    "normalize-css": "^2.3.1",
    "object-assign": "^4.1.0",
    "pleeease": "^4.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.0",
    "react-transform-hmr": "^1.0.0",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0"
  },
  "bin": {
    "springtunes": "server/server.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "build:css": "pleeease compile",
    "linter:js": "eslint",
    "lint": "npm run linter:js -s -- .",
    "server": "node server/server.js",
    "dev": "NODE_ENV=development npm run server",
    "start": "npm run build:css && npm run dist && NODE_ENV=production npm run server",
    "prepublish": "npm run build:css && npm run dist",
    "dist": "NODE_ENV=production webpack --config webpack.production.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(es6|js|json)$",
    "moduleNameMapper": {
      "commonUtil": "<rootDir>/client/common/src/util/index.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "globals": {
      "dataLayer": []
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/client/js/utils/createAction.js",
      "/client/js/containers/",
      "/client/js/actions/"
    ]
  },
  "analyze": true
}
